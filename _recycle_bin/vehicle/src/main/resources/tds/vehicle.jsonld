{
    "@context": [
        "http://w3c.github.io/wot/w3c-wot-td-context.jsonld",
        "http://w3c.github.io/wot/w3c-wot-common-context.jsonld"
    ],
    "@type": [
        "Thing",
        "vehicle"
    ],
    "name": "Smart vehicle",
    "interactions": [
        {
            "name": "lock door",
            "@type": [
                "Action",
                "LockCommand"
            ],
            "inputData": {
                "type": "boolean"
            },
            "link": [
                {
                    "href": "coap://example.org/vehicle/lock",
                    "mediaType": "application/json"
                }
            ]
        },
        {
            "name": "unlock door",
            "@type": [
                "Action",
                "UnlockCommand"
            ],
            "inputData": {
                "type": "boolean"
            },
            "link": [
                {
                    "href": "coap://example.org/vehicle/unlock",
                    "mediaType": "application/json"
                }
            ]
        },
        {
            "@id": "_:mileage",
            "name": "vehicle mileage",
            "@type": [
                "Property",
                "Mileage",
                "MultiLevelState"
            ],
            "outputType": {
                "type": "number"
            },
            "link": [
                {
                    "href": "coap://example.org/vehicle/mileage",
                    "mediaType": "application/json"
                }
            ]
        },
        {
            "@type": [
                "Event"
            ],
            "name": "change",
            "outputData": {
                "valueType": {
                    "type": "object",
                    "properties": {
                        "engineStatus": {
                            "type": "boolean"
                        },
                        "oilLevel": {
                            "type": "number",
                            "minimun": 16,
                            "maximum": 30
                        },
                        "tyrePressure": {
                            "type": "number",
                            "minimun": 0,
                            "maximum": 8
                        }
                    }
                }
            },
            "links": [
                {
                    "href": "change",
                    "mediaType": "application/json"
                }
            ]
        }
    ]
}